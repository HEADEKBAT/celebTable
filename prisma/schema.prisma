generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model celebrities {
  id       Int     @id @default(autoincrement())
  geo      String
  name     String
  category String
  subject  String
  about    String
  cimg1    String?
  cimg2    String?
  cimg3    String?
  cimg4    String?
  cimg5    String?
}
